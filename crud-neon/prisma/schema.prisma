// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id        String   @id @default(uuid())
  name      String
  email     String
  phone     String
  address   String
  message   String
  createdAt DateTime @default(now())
}
